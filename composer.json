{
    "name": "romeoz/rock-sphinx",
    "description": "Object Relational Mapping (ORM) for Sphinx Search.",
    "keywords": ["sphinx", "sphinxsearch", "search engine", "search", "active record", "sphinxql"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-sphinx/issues",
        "source": "https://github.com/romeOz/rock-sphinx"
    },
