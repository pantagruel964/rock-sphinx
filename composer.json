{
  "name": "romeoz/rock-sphinx",
  "description": "Object Relational Mapping (ORM) for Sphinx Search.",
  "keywords": [
    "sphinx",
    "sphinxsearch",
    "search engine",
    "search",
    "active record",
    "sphinxql"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "email": "serggalka@gmail.com",
    "issues": "https://github.com/romeOz/rock-sphinx/issues",
    "source": "https://github.com/romeOz/rock-sphinx"
  },
  "authors": [
    {
      "name": "Ruslan",
      "email": "serggalka@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "romeoz/rock-db": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7.0",
    "romeoz/rock-dataprovider": "*@dev",
    "romeoz/rock-cache": "*@dev",
    "romeoz/rock-validate": "*@dev",
    "romeoz/rock-sanitize": "*@dev"
  },
  "suggest": {
    "romeoz/rock-dataprovider": "required for Data provider",
    "romeoz/rock-cache": "required for caching queries",
    "romeoz/rock-validate": "required for validation rules a model",
    "romeoz/rock-sanitize": "required for sanitization rules a model",
    "romeoz/rock-behaviors": "required for using behaviors a model"
  },
  "autoload": {
    "psr-4": {
      "rock\\sphinx\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.12-dev"
    }
  }
}