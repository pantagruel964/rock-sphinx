        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-db" : "0.11.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.0",
        "romeoz/rock-cache" : "0.11.*",
        "romeoz/rock-validate" : "0.10.*",
        "romeoz/rock-sanitize": "0.10.*"
    },
    "suggest": {
